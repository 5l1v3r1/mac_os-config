#! /usr/bin/env bash

# DESCRIPTION
# Installs applications.

# EXECUTION
# CheatSheet
install_zip_app "$CHEATSHEET_APP_URL" "$CHEATSHEET_APP_NAME"

# Knox
install_zip_app "$KNOX_APP_URL" "$KNOX_APP_NAME"

# OmniGraffle
install_dmg_app "$OMNIGRAFFLE_APP_URL" "$OMNIGRAFFLE_VOLUME_NAME" "$OMNIGRAFFLE_APP_NAME"

# QuickLook - Markdown
install_zip_app "$QUICK_LOOK_MARKDOWN_APP_URL" "$QUICK_LOOK_MARKDOWN_APP_NAME"

# Sublime Text URL Handler
install_zip_app "$SUBLIME_URL_HANDLER_APP_URL" "$SUBLIME_URL_HANDLER_APP_NAME"

# SurfEasy VPN
install_dmg_app "$SURF_EASY_VPN_APP_URL" "$SURF_EASY_VPN_VOLUME_NAME" "$SURF_EASY_VPN_APP_NAME"

# Vivaldi
install_dmg_app "$VIVALDI_APP_URL" "$VIVALDI_VOLUME_NAME" "$VIVALDI_APP_NAME"
